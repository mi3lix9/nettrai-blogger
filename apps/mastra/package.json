{
  "name": "@nettrai-blogger/mastra",
  "type": "module",
  "main": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./mastra": {
      "types": "./src/mastra/index.ts",
      "import": "./src/mastra/index.ts"
    }
  },
  "scripts": {
    "build": "tsdown",
    "check-types": "tsc -b",
    "compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile mastra",
    "dev": "bun run --hot src/index.ts",
    "start": "bun run dist/index.js"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.2",
    "@hono/node-server": "^1.19.6",
    "@mastra/core": "^1.0.0-beta.19",
    "@mastra/hono": "^0.1.0-beta.14",
    "dotenv": "catalog:",
    "hono": "catalog:",
    "mastra": "^1.0.0-beta.12",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@nettrai-blogger/config": "workspace:*",
    "@types/bun": "catalog:",
    "tsdown": "^0.16.5",
    "typescript": "^5"
  }
}
